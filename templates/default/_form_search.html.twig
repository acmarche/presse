<form class="d-flex" action="{{ path('presse_search') }}">
    {{ form_start(form, { 'action': path('presse_search'), 'attr': {'class': 'd-flex'}}) }}
    {{ form_errors(form) }}
    {{ form_row(form.keyword) }}
    <button class="btn btn-outline-success" type="submit">Rechercher</button>
{{ form_end(form) }}